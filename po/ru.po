#, python-format
msgid "Preset %d: %s"
msgstr ""

#: picard/script/__init__.py:149
msgid "Default file naming script"
msgstr ""

#: picard/script/__init__.py:163
msgid "[album artist]/[album]/[track #]. [title]"
msgstr ""

#: picard/script/__init__.py:179
